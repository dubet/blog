{
  "hosting": {
    "headers": [
      {
        "source": "**/*.@(webp|ttf|svg|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000"
          }
        ]
      }
    ],
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "node_modules/**"
    ]
  }
}
